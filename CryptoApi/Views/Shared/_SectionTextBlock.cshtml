@model (string title, string template, object data)

<div class="dc-section-text-block">
    <div class="row">
        <div class="col-lg-4 col-md-12">
            <h2 class="dcg-h2 p-0 text-left">@Model.title</h2>
        </div>
        <div class="col-lg-8 col-md-12"> 
            @await Html.PartialAsync(Model.template, Model.data)
        </div>
    </div>
</div>