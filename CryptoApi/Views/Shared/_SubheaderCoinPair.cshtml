@*@model CCoinDataVM*@
<div class="dc-subheader-coin-pair">
    <div class="row">
        <div class="col">
            @await Html.PartialAsync("_Breadcrumbs", new[] { ("Cryptocurrency Exchange Pairs", Url.RouteUrl(new { Action = "Index", Controller = "CoinPairs" })), ("BTC" + " to " + "BTC", "") })
        </div>
    </div>
    <div class="row">
        <div class="col">
            <h1 class="dcg-h1">Exchange <strong>DODO</strong> to <strong>USD</strong></h1>
            @await Html.PartialAsync("_CoinWidget")
        </div>
        <div class="col">
            @await Html.PartialAsync("_ExchangeWidget", ("btc", "eth"))
        </div>
    </div>
</div>